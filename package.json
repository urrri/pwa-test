{
  "name": "pwa-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.11",
    "sw-precache": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "buildx": "react-scripts build && sw-precache --root='build/' --static-file-globs='build/**/!(*map*)'",
    "deploy": "gh-pages -d build",
    "homepage": "./"
  }
}
